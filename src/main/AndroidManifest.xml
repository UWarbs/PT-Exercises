<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.warbis.ptexercises" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayBackActivity"
            android:label="@string/title_activity_display_back"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.warbis.ptexercises.MainActivity" />
        </activity>
        <activity
            android:name=".DisplayKneeActivity"
            android:label="@string/title_activity_display_knee"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.warbis.ptexercises.MainActivity" />
        </activity>
        <activity
            android:name=".DisplayAnkleActivity"
            android:label="@string/title_activity_display_ankle"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.warbis.ptexercises.MainActivity" />
        </activity>
        <activity
            android:name=".DisplayNeckActivity"
            android:label="@string/title_activity_display_neck"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.warbis.ptexercises.MainActivity" />
        </activity>
        <activity
            android:name=".DisplayShoulderActivity"
            android:label="@string/title_activity_display_shoulder"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.warbis.ptexercises.MainActivity" />
        </activity>
    </application>

</manifest>
